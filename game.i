void updateRecordPlayer() {
    recordPlayer.aniCounter++;
    recordPlayer.curFrame += recordPlayer.aniCounter/8;
    if (recordPlayer.aniCounter >= 8) {
        recordPlayer.aniCounter = 0;
    }


    shadowOAM[recordPlayer.OAMIndex].attr0 = recordPlayer.row;
    shadowOAM[recordPlayer.OAMIndex].attr1 = recordPlayer.col | (1 << 14);
    if (recordPlayer.flip) {
        shadowOAM[recordPlayer.OAMIndex].attr1 |= (1 << 12);
    }
    shadowOAM[recordPlayer.OAMIndex].attr2 = (((recordPlayer.curFrame % 4) * 2)*32 + (recordPlayer.aniState)) |
                                            ((1) << 12);

    if (!recordPlayer.showInteraction || player.level == 0) {
        shadowOAM[recordPlayer.OAMIndex+1].attr0 = (2 << 8);
    } else {
        shadowOAM[recordPlayer.OAMIndex+1].attr0 = recordPlayer.row - 8 | (1 << 14);
        shadowOAM[recordPlayer.OAMIndex+1].attr1 = recordPlayer.col | (0 << 14);
        shadowOAM[recordPlayer.OAMIndex+1].attr2 = (((recordPlayer.curFrame % 4))*32 + (recordPlayer.aniState - 4)) |
                                            ((2) << 12);
    }

}

