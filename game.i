void updatePlayer() {

    if (player.playerControls) {
        if (player.isCheating) {
            player.vx = 0;
            player.vy = 0;
            if ((~((*(volatile unsigned short *)0x04000130)) & ((1 << 6)))) {
                player.vy -= 200;
            }
            if ((~((*(volatile unsigned short *)0x04000130)) & ((1 << 7)))) {
                player.vy += 200;
            }
            if ((~((*(volatile unsigned short *)0x04000130)) & ((1 << 5)))) {
                player.vx -= 200;
                player.direction = LEFT;
            }
            if ((~((*(volatile unsigned short *)0x04000130)) & ((1 << 4)))) {
                player.vx += 200;
                player.direction = RIGHT;
            }
        } else {
            player.aniState = IDLE;

            player.vy += 70;

            if ((!(~(oldButtons) & ((1 << 6))) && (~buttons & ((1 << 6)))) && player.canJump) {
                player.vy -= 900;
                player.canJump = 0;
            }
            if ((~((*(volatile unsigned short *)0x04000130)) & ((1 << 5)))) {
                player.direction = LEFT;
                player.aniState = RUNNING;
                player.vx -= 150;
                if (player.vx <= -600) {
                    player.vx = -600;
                }
            }
            if ((~((*(volatile unsigned short *)0x04000130)) & ((1 << 4)))) {
                player.direction = RIGHT;
                player.aniState = RUNNING;
                player.vx += 150;
                if (600 <= player.vx) {
                    player.vx = 600;
                }
            }


            if (0 < player.vx) {
                player.vx -= 50;
                if (player.vx < 0) {
                    player.vx = 0;
                }
            } else if (player.vx < 0) {
                player.vx += 50;
                if (0 < player.vx) {
                    player.vx = 0;
                }
            }

            if (freeToMove(((player.col) >> 8), ((player.row) >> 8),
                        player.width-1, player.height, 0, -1, curColMap)) {
                if (!freeToMove(((player.col) >> 8), ((player.row) >> 8),
                            player.width, player.height-1, 1, 0, curColMap)) {
                    player.lastWallTouch = RIGHT;
                    player.direction = LEFT;
                    player.aniState = ONWALL;
                    if (player.lastJumpSide != player.lastWallTouch) {
                        player.canJump = 1;
                        player.vy = 0;
                        player.vy += 70;
                        player.vy -= 50;
                        if ((!(~(oldButtons) & ((1 << 6))) && (~buttons & ((1 << 6))))) {
                            player.vy -= 900;
                            player.vx -= 600;
                            player.lastJumpSide = RIGHT;
                        }
                        if ((~((*(volatile unsigned short *)0x04000130)) & ((1 << 7)))) {
                            player.vy += 200;
                        }
                    }
                } else if (!freeToMove(((player.col) >> 8), ((player.row) >> 8),
                            player.width, player.height-1, -1, 0, curColMap)) {
                    player.lastWallTouch = LEFT;
                    player.direction = RIGHT;
                    player.aniState = ONWALL;
                    if (player.lastJumpSide != player.lastWallTouch) {
                        player.canJump = 1;
                        player.vy = 0;
                        player.vy += 70;
                        player.vy -= 50;
                        if ((!(~(oldButtons) & ((1 << 6))) && (~buttons & ((1 << 6))))) {
                            player.vy -= 900;
                            player.vx += 600;
                            player.lastJumpSide = LEFT;
                        }
                        if ((~((*(volatile unsigned short *)0x04000130)) & ((1 << 7)))) {
                            player.vy += 200;
                        }
                    }

                } else {
                    player.canJump = 0;
                }
            }

            if (freeToMove(((player.col) >> 8), ((player.row) >> 8),
                player.width, player.height, 1, 1, curColMap) &&
                freeToMove(((player.col) >> 8), ((player.row) >> 8),
                player.width, player.height, -1, -1, curColMap)) {
                if (0 < player.vy) {
                    player.aniState = FALLING;
                } else if (player.vy < 0) {
                    player.aniState = JUMPING;
                }
            }
        }



        if (freeToMove(((player.col) >> 8), ((player.row) >> 8),
                    player.width, player.height-1, ((player.vx) >> 8), 0, curColMap)) {
            player.col += player.vx;
        } else {

            int dCol = 0;
            if (player.vx < 0) {
                for (int i = 0; i >= player.vx; i--) {
                    if (freeToMove(((player.col) >> 8), ((player.row) >> 8),
                                player.width, player.height-1, ((i) >> 8), 0, curColMap)) {
                        dCol = i;
                    }
                }
            }
            if (0 < player.vx) {
                for (int i = 0; i <= player.vx; i++) {
                    if (freeToMove(((player.col) >> 8), ((player.row) >> 8),
                                player.width, player.height-1, ((i) >> 8), 0, curColMap)) {
                        dCol = i;
                    }
                }
            }
            if (dCol != player.vx) {
                player.vx = 0;
                player.col += dCol;
            } else {
                player.col += player.vx;
            }
        }


        if (freeToMove(((player.col) >> 8), ((player.row) >> 8),
                    player.width-1, player.height, 0, ((player.vy) >> 8), curColMap)) {
            player.row += player.vy;
        } else {
            player.vy = 0;
            player.canJump = 1;
            player.lastJumpSide = ONGROUND;
            player.lastWallTouch = ONGROUND;
        }


        if (checkWinDie(((player.col) >> 8), ((player.row) >> 8),
            player.width, player.height, curColMap) == 2 && !player.isCheating) {
            player.playerControls = 0;
            player.aniState = DYING;
            player.aniCounter = 0;
            player.curFrame = 0;
            player.lives--;
            playEffect(deathEffect_data, deathEffect_length);
        } else if (checkWinDie(((player.col) >> 8), ((player.row) >> 8),
            player.width, player.height, curColMap) == 3) {
            recordPlayer.showInteraction = 1;
            if ((!(~(oldButtons) & ((1 << 0))) && (~buttons & ((1 << 0))))) {
                player.level++;
                recordPlayer.showInteraction = 0;


                initPlayer();
                initRecordPlayer();
                initCheat();
            }
        } else {
            recordPlayer.showInteraction = 0;
        }
    }

    if ((!(~(oldButtons) & ((1 << 8))) && (~buttons & ((1 << 8))))) {
        if (player.isCheating2) {
            player.isCheating2 = 0;
        } else {
            player.isCheating2 = 1;
        }
    }

    if ((!(~(oldButtons) & ((1 << 2))) && (~buttons & ((1 << 2))))) {
        if (player.isCheating) {
            player.isCheating = 0;
        } else {
            player.isCheating = 1;
        }
    }

    if (player.isCheating) {
        player.aniState = CHEATING;
    }


    player.aniCounter++;
    player.curFrame += player.aniCounter/8;
    if (player.aniCounter >= 8) {
        player.aniCounter = 0;
        if (player.aniState == DYING && player.curFrame == 4) {
            player.playerControls = 1;
            respawnPlayer();
        }
    }



    shadowOAM[player.OAMIndex].attr0 = ((player.row) >> 8) | (2 << 14);
    shadowOAM[player.OAMIndex].attr1 = ((player.col) >> 8) | (0 << 14);
    if (player.direction == LEFT) {
        shadowOAM[player.OAMIndex].attr1 |= (1 << 12);
    }
    shadowOAM[player.OAMIndex].attr2 = (((player.curFrame % 4) * 2)*32 + (player.aniState));
}

